generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Trasactions {
  id        String   @unique @default(uuid())
  userId    String
  title     String
  amount    Decimal  @db.Decimal(10, 2)
  category  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
